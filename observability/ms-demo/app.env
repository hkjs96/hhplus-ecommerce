# Spring (docker)
SPRING_PROFILES_ACTIVE=local
MYSQL_HOST=mysql
MYSQL_PORT=3306
MYSQL_DB=ecommerce
MYSQL_USER=ecommerce_user
MYSQL_PASSWORD=ecommerce_pass
SPRING_DATA_REDIS_HOST=redis
SPRING_DATA_REDIS_PORT=6379

# App logs (OTel agent log correlation MDC)
LOGGING_PATTERN_CONSOLE=%d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - trace_id=%X{trace_id} span_id=%X{span_id} %msg%n

